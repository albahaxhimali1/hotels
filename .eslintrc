{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "object-curly-spacing": "off",
    "space-before-function-paren": 0,
    "computed-property-even-spacing": "off"
  }
}
